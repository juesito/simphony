<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <description>A la pagina principal</description>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>toindex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>toUsers</from-outcome>
            <to-view-id>/catalogs/users/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>de catalogo de usuarios</description>
        <from-view-id>/catalogs/users/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addUser</from-outcome>
            <to-view-id>/catalogs/users/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>tousers</from-outcome>
            <to-view-id>/catalogs/users/users.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
