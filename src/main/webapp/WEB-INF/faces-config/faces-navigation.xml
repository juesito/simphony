<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <description>A la pagina principal</description>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>toindex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>de catalogo de usuarios</description>
            <from-outcome>toUsers</from-outcome>
            <to-view-id>/catalogs/users/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>        
            <from-outcome>addUser</from-outcome>
            <to-view-id>/catalogs/users/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>de catalogo de usuarios</description>
            <from-outcome>toVendors</from-outcome>
            <to-view-id>/catalogs/vendors/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>        
            <from-outcome>addVendor</from-outcome>
            <to-view-id>/catalogs/vendors/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addPopulation</from-outcome>
            <to-view-id>/catalogs/populations/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>toPopulations</from-outcome>
            <to-view-id>/catalogs/populations/list.xhtml</to-view-id>
        </navigation-case>
                <navigation-case>
            <description>de catalogo de agremiados</description>
            <from-outcome>toAssociates</from-outcome>
            <to-view-id>/catalogs/associates/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>        
            <from-outcome>addAssociate</from-outcome>
            <to-view-id>/catalogs/associates/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>

        <navigation-case>
            <from-outcome>addWorkCenter</from-outcome>
            <to-view-id>/catalogs/workCenters/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>toWorkCenter</from-outcome>
            <to-view-id>/catalogs/workCenters/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addSalePoint</from-outcome>
            <to-view-id>/catalogs/salePoints/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>toSalePoint</from-outcome>
            <to-view-id>/catalogs/salePoints/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addCost</from-outcome>
            <to-view-id>/catalogs/costs/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>toCosts</from-outcome>
            <to-view-id>/catalogs/costs/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addBus</from-outcome>
            <to-view-id>/catalogs/buses/add.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>toBus</from-outcome>
            <to-view-id>/catalogs/buses/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toUserTypes</from-outcome>
            <to-view-id>/catalogs/userTypes/list.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addUserTypes</from-outcome>
            <to-view-id>/catalogs/userTypes/add.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toLogin</from-outcome>
            <to-view-id>/include/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>toSale</from-outcome>
            <to-view-id>/sale.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>
</faces-config>
